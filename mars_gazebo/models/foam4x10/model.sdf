            <uri>model://foam4x10/meshes/foam4x10.stl</uri>
            <scale>0.001 0.001 0.001</scale>
          </mesh>
        </geometry>
      </collision>
      <pose>0 0 0 0 0 0</pose>
    </link>
    <static>1</static>
